programa { inteiro media, n1, n2, n3
	funcao inicio() {
        escreva("Qual foi a nota do primeiro bimestre?", "\n")
        leia(n1)
        escreva("Qual foi a nota do segundo bimestre?", "\n")
        leia(n2)
        escreva("Qual foi a nota do terceiro bimestre?", "\n")
        leia(n3)
        media=(n1+n2+n3)/3
        escreva("A sua nota média foi ", media, ".")
	}
}
