programa {
    real fone
    cadeia endereco, nome
    
	funcao inicio() {
		escreva("Entre com seu nome, endereço e número de telefone.", "\n")
		leia(nome)
		escreva("Seu nome é: ", nome, "\n")
		leia(endereco)
		escreva("Seu endereço é: ", endereco, "\n")
		leia(fone)
		escreva("Seu número de telefone é: ", fone, "\n")
		
		escreva("Seu nome é: ", nome, ",", "\n","Seu endereço é: ", endereco, ",", "\n","Seu número de telefone é: ", fone, ",", "\n" )
		
	}
}
